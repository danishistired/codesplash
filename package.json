{
  "name": "codesplash",
  "displayName": "Code Splash",
  "description": "Splash your code to others and set up their environment directly, just like docker but better.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codeShareContainer",
          "title": "CodeShare",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "codeShareContainer": [
        {
          "type": "webview",
          "icon": "media/icon2.svg",
          "id": "codeShareView",
          "name": "Code Share Panel"
        }
      ]
    },
    "commands": [
      {
        "command": "codeShare.sendCode",
        "title": "Send Code"
      },
      {
        "command": "codeShare.receiveCode",
        "title": "Receive Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "firebase": "^11.9.1",
    "form-data": "^4.0.3"
  }
}
